#plan
	padding 90px 0
	background-color: #f9f3ec
	background-image url(../img/backs/plan.png)
	background-repeat no-repeat
	background-position: center bottom
.module
	border-radius: 50px;
	box-shadow: 0px 1px 44.16px 1.84px rgba(0, 0, 0, 0.1);
	padding 50px
	background-color #fff
	width 100%
	max-width 650px
	margin 0 auto 40px
	&-2
		min-height 675px
	&-3
		min-height 489px
	&-title
		font-size: 24px
		font-family: MBk
		font-weight: 900
		text-transform uppercase
		small
			text-transform: none
			display block
			font-size: 20px
			font-family: MBd
			font-weight: bold
	&-list
		margin 20px 0 30px
		li
			list-style: none
			span
				font-family: MBd
				font-weight: bold